#! /bin/sh

set -e

case "$1" in
    upgrade|failed-upgrade|abort-install|abort-upgrade)
	;;
    remove|purge|disappear)
	if [ -x /usr/sbin/remove-shell ] && [ -f /etc/shells ]; then
	    /usr/sbin/remove-shell /bin/vbash
	fi
	;;
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
	;;
esac

#DEBHELPER#

exit 0
